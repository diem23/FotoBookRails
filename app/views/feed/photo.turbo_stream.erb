<%= turbo_stream.append "photos" do %>
  <% p "come intoooo feed/index.turbo_stream.erb" %>
  <%= render partial: 'shared/photo_card', collection: @list_photos, ele: ele, index: index, have_follow: false , from_page: "feed"%>
<% end %>

<% if @list_photos.next_page %>
    <%= turbo_stream.replace "load_more" do %>
        <%= turbo_frame_tag "load-more", src: root_path(page: @list_photos.next_page, format: :turbo_stream), loading: :lazy %>
    <% end %>
<% end %>
