<%= turbo_stream.append "photos" do%>
    
    <% @resources.each_with_index do |ele,index|%>
        <%=render 'shared/photo_card', ele: ele, index: index , have_follow: true, from_page: "discover"%>
    <% end %>
<%end%>

<% if @resources.next_page%>
    <%= turbo_stream.replace "load_more" do%>
        <%= turbo_frame_tag "load_more", src:discover_photo_path(page:@resources.next_page ,format: :turbo_stream), loading: :lazy %>
    <%end%>
<%end%>

<%= turbo_stream.update "current_page", @resources.current_page %>