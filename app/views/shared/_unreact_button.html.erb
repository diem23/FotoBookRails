<% ele = local_assigns[:ele] %>
<% list_to_check = local_assigns[:list_to_check] %>
<% inside_photo = local_assigns[:inside_photo]%>

<button class="button-class   <%=(inside_photo==true)? "unreact-photo-btn unreact-photo-btn-#{ele.id}" : "unreact-album-btn unreact-album-btn-#{ele.id}"%>" form="form-class" >
    <i class=" fa-solid fa-heart fa-xs heart unheart"></i>
    <p class="d-none"><%=ele.id%></p>
</button>
<p class="counting-likes"><%=ele.numOfLikes%></p>
<script>

document.querySelectorAll('.unreact-photo-btn').forEach((btn)=>{
    btn.addEventListener('click',function stopPropagation(event){
        console.log("onclick like")
        event.stopPropagation();
        debounceReact(unreactPhotoApi)(btn.querySelector('p').innerHTML)
    })
}) 

document.querySelectorAll('.unreact-album-btn').forEach((btn)=>{
    btn.addEventListener('click',function stopPropagation(event){
        console.log("onclick like")
        event.stopPropagation();
        debounceReact(unreactAlbumApi)(btn.querySelector('p').innerHTML)
    })
})
</script>